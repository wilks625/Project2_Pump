{{#each profile as |profiles| }}
<h1>{{location}}</h1>
{{/each}}


<h3>{{user.firstname}} {{user.lastname}}</h3>
<p>{{user.username}}</p>
<p>Activities: {{activities}}</p>  
<p>Bio: {{bio}}</p>  
<p>Age: {{age}}</p>  
<p>Location: {{location}}</p>
<p>snapchat: {{snapchat}}</p>  
